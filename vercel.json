{
  "version": 2,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/enrich",
      "dest": "/api.py"
    },
    {
      "src": "/webhook", 
      "dest": "/api.py"
    },
    {
      "src": "/health",
      "dest": "/api.py"
    }
  ]
}
